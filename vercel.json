{
  "rewrites": [
    {
      "source": "/api/user-sites/:id",
      "destination": "/api/user-sites/[id]"
    },
    {
      "source": "/api/users/:id",
      "destination": "/api/users/[id]"
    },
    {
      "source": "/api/users/:id/password",
      "destination": "/api/users/[id]/password"
    },
    {
      "source": "/api/gsc-sync/jobs/:jobId",
      "destination": "/api/gsc-sync/jobs/[jobId]"
    }
  ]
}
